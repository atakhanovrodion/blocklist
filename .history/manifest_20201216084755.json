{
  "name": "igonrlist",
  "version": "0.1",

  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "*://*/*",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "web_accessible_resources": ["blockedSite.html"],

  "manifest_version": 2
}
