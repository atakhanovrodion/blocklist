{
  "name": "igonrlist",
  "version": "0.1",

  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "*://*/*",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentScript.js"],
      "run_at": "document_start"
    }
  ],

  "manifest_version": 2
}
